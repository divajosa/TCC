<?php
class Responsavel {
  private $id;
  private $nome;
  private $sobrenome;
  private $data_nascimento;
  private $cpf;
  private $e_mail;
  private $senha;
  private $endereco;
  private $numero;
  private $complemento;
  private $bairro;
  private $cidade;
  private $estado;
  private $cep;
  private $celular;
